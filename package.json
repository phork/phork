    "about-eslint": "echo 'ğŸ¤–ğŸ–ï¸ Run eslint and fix the problems' && exit 0",
    "about-gen-icons": "echo 'ğŸ§° Generate React components from the SVG icons' && exit 0",
    "about-gen-json-vars": "echo 'ğŸ§° Create JSON config files out of the PostCSS vars' && exit 0",
