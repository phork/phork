    "about-validate": "echo 'ğŸ¤– Validates the files with eslint and stylelint' && exit 0",
    "about-watch": "echo 'ğŸ¤–ğŸ‘ï¸â€ğŸ—¨ï¸ Watch for typescript bugs' && exit 0",
    "build-deps": "npm-run-all --serial clean-icons gen-icons gen-json-vars",
    "build": "NODE_ENV=production && npx webpack --config webpack.prod.js",
    "clean-build": "rimraf build",
    "clean-icons": "rimraf src/icons && mkdirp src/icons",
    "deploy": "yarn build-deps && yarn build && yarn deploy",
    "eslint": "npx eslint \"{src,__tests__}/**/*.{js,json,ts,tsx,mdx}\" --fix",
    "gen-icons": "node ./scripts/generate-svg-icons.js",
    "gen-json-vars": "node ./scripts/generate-postcss-vars-json.js",
    "postinstall": "husky install",
    "pre-commit": "npx lint-staged",
    "prepare": "husky install",
    "prettier": "npx prettier \"./**/*.{js,json,ts,tsx}\" --write",
    "start-express-dev": "yarn build-deps && npx webpack serve --config webpack.dev.js --analyze",
    "start-express-prod": "yarn start-express-dev --node-env production",
    "start": "yarn start-express-dev",
    "stylelint": "npx stylelint \"src/**/*.css\" --fix",
    "test": "npx jest",
    "test-watch": "npx jest --watch",
    "validate": "npx eslint \"src/**/*.{js,json,ts,tsx}\" && npx stylelint \"src/**/*.css\"",
    "watch": "tsc --watch --noEmit --project './tsconfig.json'"
  },
  "files": [
    "build/*"
  ],
  "sideEffects": [
    "*.css"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "yarn prettier",
      "yarn eslint",
      "git add"
    ],
    "*.css": [
      "yarn stylelint",
      "git add"
    ]
